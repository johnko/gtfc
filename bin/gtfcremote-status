#!/bin/sh

# Compares the last git log of gitsync in HOSTS
# Usage
#  env MYUSER=urep HOSTS="one two three" $0


if [ "x" = "x${MYUSER}" ]; then
    MYUSER="`whoami`"
fi

for i in ${HOSTS} ; do
    printf '%s ' $i
    for k in `seq ${#i} 10` ; do
        printf ' ';
    done
    ssh ${MYUSER}@$i 'cd gitcluster && git log --oneline --decorate -1 | sed "s/master//g;s:/,::g"'
done
